    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.4.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4"
  },
