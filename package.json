    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-subset": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^8.2.2",
    "@types/node-rsa": "^1.1.0",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
